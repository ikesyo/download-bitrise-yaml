{
  "name": "download-bitrise-yaml",
  "version": "0.1.0",
  "private": true,
  "description": "GitHub Action for downloading bitrise.yml",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "ikesyo",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/http-client": "^1.0.11"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.5",
    "@typescript-eslint/parser": "^4.28.4",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.31.0",
    "eslint-plugin-github": "^4.1.5",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
}
