{
  "name": "download-bitrise-yaml",
  "version": "0.1.0",
  "private": true,
  "description": "GitHub Action for downloading bitrise.yml",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "ikesyo",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.5.0",
    "@actions/http-client": "^1.0.11"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^14.18.9",
    "@typescript-eslint/parser": "^4.33.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-jest": "^24.7.0",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^27.1.2",
    "typescript": "^4.3.5"
  }
}
