{
  "name": "download-bitrise-yaml",
  "version": "0.1.0",
  "private": true,
  "description": "GitHub Action for downloading bitrise.yml",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "YourNameOrOrganization",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/http-client": "^1.0.8"
  },
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "@types/node": "^12.12.53",
    "@typescript-eslint/parser": "^3.7.1",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^3.4.1",
    "eslint-plugin-jest": "^23.19.0",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "js-yaml": "^3.14.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  }
}
